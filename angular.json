{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-ode-sijil": {
      "projectType": "library",
      "root": "projects/ngx-ode-sijil",
      "sourceRoot": "projects/ngx-ode-sijil/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-ode-sijil/tsconfig.lib.json",
            "project": "projects/ngx-ode-sijil/ng-package.json"
          }
        ,          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-ode-sijil/tsconfig.lib.prod.json"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
},
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-ode-sijil/src/test.ts",
            "tsConfig": "projects/ngx-ode-sijil/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-ode-sijil/karma.conf.js"
          }
        }
      }
    },
    "ngx-ode-ui": {
      "projectType": "library",
      "root": "projects/ngx-ode-ui",
      "sourceRoot": "projects/ngx-ode-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-ode-ui/tsconfig.lib.json",
            "project": "projects/ngx-ode-ui/ng-package.json"
          }
        ,          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-ode-ui/tsconfig.lib.prod.json"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
},
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-ode-ui/src/test.ts",
            "tsConfig": "projects/ngx-ode-ui/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-ode-ui/karma.conf.js"
          }
        }
      }
    },
    "ngx-ode-core": {
      "projectType": "library",
      "root": "projects/ngx-ode-core",
      "sourceRoot": "projects/ngx-ode-core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-ode-core/tsconfig.lib.json",
            "project": "projects/ngx-ode-core/ng-package.json"
          }
        ,          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-ode-core/tsconfig.lib.prod.json"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
},
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-ode-core/src/test.ts",
            "tsConfig": "projects/ngx-ode-core/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-ode-core/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
